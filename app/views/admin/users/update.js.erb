<% if @user.errors.size == 0 %>
  $("#dialog_update_user").dialog("close");
  $("#user_role").text("<%= @user.role %>");
  $("#user_preferred_email").text("<%= @user.preferred_email %>");
<% else %>
  alert("<%= escape_javascript(render('shared/error', object: @user, title: 'Error updating user:')) %>")
<% end %>
