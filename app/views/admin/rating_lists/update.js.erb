<% if @rating_list.errors.size == 0 %>
  $("#tournament_cut_off").text("<%= @rating_list.tournament_cut_off %>");
  $("#payment_cut_off").text("<%= @rating_list.payment_cut_off %>");
  $("#dialog_update_rating_list").dialog("close");
<% else %>
  alert("<%= j render('shared/error', object: @rating_list, title: 'Error updating rating list:') %>");
<% end %>
