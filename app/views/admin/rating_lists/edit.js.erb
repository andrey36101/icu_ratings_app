$("#dialog_update_rating_list")
  .html("<%= j render('form') %>")
  .dialog("option", "title", "Update Rating List")
  .dialog("open");
$("#rating_list_tournament_cut_off").datepicker({
  dateFormat: 'yy-mm-dd',
  autoSize: true
});
$("#rating_list_payment_cut_off").datepicker({
  dateFormat: 'yy-mm-dd',
  autoSize: true
});
$("#rating_list_tournament_cut_off, #rating_list_payment_cut_off").blur(); // otherwise datepicker has problems in Chrome
