= form_tag admin_rating_lists_path, method: "get", remote: true, id: "list_form" do
  = hidden_field_tag :per_page, params[:per_page]
  %table{class: "invisible"}
    %tr
      %th= label_tag :year
    %tr
      %td= select_tag :year, options_for_select(rating_list_year_menu("Any"), params[:year]), onchange: "$.rails.handleRemote($('#list_form'))"
