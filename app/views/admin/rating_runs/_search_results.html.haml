%table
  %tr
    %th Time Ago
    %th Start Tournament
    %th #
  - if @rating_runs.count == 0
    %tr
      %td{colspan: 3, class: "centered"} No matches
  - else
    - @rating_runs.matches.each do |rating_run|
      %tr
        %td= link_to time_ago_in_words(rating_run.created_at, true), [:admin, rating_run]
        %td= rating_run.start_tournament_name
        %td= "#{rating_run.start_tournament_rorder}-#{rating_run.last_tournament_rorder}"
  - if @rating_runs.multi_page
    %tr
      %th{class: "centered", colspan: 3}= pagination_links(@rating_runs)
