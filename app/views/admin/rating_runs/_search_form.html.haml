= form_tag admin_rating_runs_path, method: "get", remote: true do
  = hidden_field_tag :per_page, params[:per_page]
  %table{class: "invisible"}
    %tr
      %th= label_tag :status
      %th
    %tr
      %td= select_tag :status, options_for_select([["Any", ""], ["Yes", 1], ["No", 0]], params[:success])
      %td= submit_tag "Search"
