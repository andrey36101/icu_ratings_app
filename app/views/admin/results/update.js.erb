<% if @player && @tournament %>
  $("#results").html("<%= j render('admin/players/results') %>");
  $("#score").html("<%= score_html(@player.score, rounds: @player.results.size) %>");
  $("#dialog_update_player").dialog("close");
<% else %>
  alert("<%= j render('shared/error', object: @result, title: 'Error updating result:') %>")
<% end %>
